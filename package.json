{
  "name": "quick-search-plus",
  "displayName": "quick-search-plus",
  "description": "this is a useful quick search plugin for vscode!",
  "version": "0.0.2",
  "publisher": "Bigonion",
  "icon": "assets/img/icon.png",
  "bugs": {"url": "https://github.com/LiWeny16/quick-search-plus.git/issues"},
  "repository": {
		"type": "git",
		"url": "https://github.com/LiWeny16/quick-search-plus.git"
	},
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-search-plus.qs",
        "title": "quick Search",
        "shortTitle": "qs"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "fast-xml-parser": "^4.2.2",
    "https-proxy-agent": "^6.2.0"
  }
}
